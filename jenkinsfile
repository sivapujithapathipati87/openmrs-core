pipeline
{
    agent{
        label 'MAVEN'
    }
    option{
        timeout(time: 30, units: 'minutes')
    }
    triggers{
        pollSCM('* * * * *')
    }
    stages{
        stage('git')
        {
            steps{
            git url: 'https://github.com/sivapujithapathipati87/openmrs-core.git',
          }
        }
      }
      stage('build')
      {
        steps{
            sh 'mvn clean package'
        }
      }
}